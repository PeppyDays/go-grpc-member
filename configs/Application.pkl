@go.Package{ name = "github.com/peppydays/go-grpc-member/configs" }
module Application

import "package://pkg.pkl-lang.org/pkl-go/pkl.golang@0.6.0#/go.pkl"

// TODO: Troubleshoot the why environment variables cannot be read

/// The URL to listen on
host: String = read?("env:APP_HOST") ?? "127.0.0.1"
 
/// The port to listen on
port: UInt16(isBetween(0, 65535)) = read?("env:APP_PORT")?.toInt() ?? 9090

